@Article{Peters2012,
  Title                    = {Accurate whole-genome sequencing and haplotyping from 10 to 20 human cells},
  Author                   = {Peters, Brock A. and Kermani, Bahram G. and Sparks, Andrew B. and Alferov, Oleg and Hong, Peter and Alexeev, Andrei and Jiang, Yuan and Dahl, Fredrik and Tang, Y Tom and Haas, Juergen and Robasky, Kimberly and Zaranek, Alexander Wait and Lee, Je-Hyuk and Ball, Madeleine Price and Peterson, Joseph E. and Perazich, Helena and Yeung, George and Liu, Jia and Chen, Linsu and Kennemer, Michael I. and Pothuraju, Kaliprasad and Konvicka, Karel and Tsoupko-Sitnikov, Mike and Pant, Krishna P. and Ebert, Jessica C. and Nilsen, Geoffrey B. and Baccash, Jonathan and Halpern, Aaron L. and Church, George M. and Drmanac, Radoje},
  Journal                  = {Nature},
  Year                     = {2012},

  Month                    = {Jul},
  Number                   = {7406},
  Pages                    = {190--195},
  Volume                   = {487},

  Abstract                 = {Recent advances in whole-genome sequencing have brought the vision of personal genomics and genomic medicine closer to reality. However, current methods lack clinical accuracy and the ability to describe the context (haplotypes) in which genome variants co-occur in a cost-effective manner. Here we describe a low-cost DNA sequencing and haplotyping process, long fragment read (LFR) technology, which is similar to sequencing long single DNA molecules without cloning or separation of metaphase chromosomes. In this study, ten LFR libraries were made using only â¼100âpicograms of human DNA per sample. Up to 97\% of the heterozygous single nucleotide variants were assembled into long haplotype contigs. Removal of false positive single nucleotide variants not phased by multiple LFR haplotypes resulted in a final genome error rate of 1 in 10âmegabases. Cost-effective and accurate genome sequencing and haplotyping from 10-20 human cells, as demonstrated here, will enable comprehensive genetic studies and diverse clinical applications.},
  Doi                      = {10.1038/nature11236},
  Institution              = {Complete Genomics, Inc., 2071 Stierlin Court, Mountain View, California 94043, USA. bpeters@completegenomics.com},
  Keywords                 = {Alleles; Cell Line; Female; Gene Silencing; Genetic Variation; Genome, Human; Genomics, methods; Haplotypes; Humans; Mutation; Reproducibility of Results; Sequence Analysis, DNA, economics/methods/standards},
