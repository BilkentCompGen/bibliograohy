  Pmid                     = {21121036},
  Timestamp                = {2012.03.07},
  Url                      = {http://psb.stanford.edu/psb-online/proceedings/psb11/halldorsson.pdf}
}

@Article{Handsaker2011,
  author      = {Handsaker, Robert E. and Korn, Joshua M. and Nemesh, James and McCarroll, Steven A.},
  title       = {Discovery and genotyping of genome structural polymorphism by sequencing on a population scale},
  year        = {2011},
  language    = {eng},
