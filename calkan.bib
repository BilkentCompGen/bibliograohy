  Pmid                     = {17158514},
  Timestamp                = {2008.03.02},
  Url                      = {http://dx.doi.org/10.1093/bioinformatics/btl629}
}

@Article{Watson2013,
  Title                    = {Complete haplotype sequence of the human immunoglobulin heavy-chain variable, diversity, and joining genes and characterization of allelic and copy-number variation},
  Author                   = {Watson, Corey T. and Steinberg, Karyn M. and Huddleston, John and Warren, Rene L. and Malig, Maika and Schein, Jacqueline and Willsey, A Jeremy and Joy, Jeffrey B. and Scott, Jamie K. and Graves, Tina A. and Wilson, Richard K. and Holt, Robert A. and Eichler, Evan E. and Breden, Felix},
  Journal                  = {Am J Hum Genet},
