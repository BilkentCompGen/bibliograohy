  issn-linking    = {1066-5277},
  keywords        = {Diploidy; Genetics, Population, methods; Genome, Human, genetics; Haplotypes, genetics; High-Throughput Nucleotide Sequencing, methods; Humans; Polymorphism, Single Nucleotide, genetics; Sequence Analysis, DNA, methods; algorithms; combinatorial optimization; dynamic programming; haplotypes; next generation sequencing},
