}

@Article{Nasser2014,
  author          = {Nasser, Waleed and Beres, Stephen B and Olsen, Randall J and Dean, Melissa A and Rice, Kelsey A and Long, S Wesley and Kristinsson, Karl G and Gottfredsson, Magnus and Vuopio, Jaana and Raisanen, Kati and Caugant, Dominique A and Steinbakk, Martin and Low, Donald E and McGeer, Allison and Darenberg, Jessica and Henriques-Normark, Birgitta and Van Beneden, Chris A and Hoffmann, Steen and Musser, James M},
