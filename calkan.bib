@Article{Dal2014,
  Title                    = {Early postzygotic mutations contribute to de novo variation in a healthy monozygotic twin pair},
  Author                   = {Dal, GulÅah M. and Erguner, Bekir and Sagiroglu, Mahmut S. and Yuksel, Bayram and Onat, Onur Emre and Alkan, Can and Ozcelik, Tayfun},
