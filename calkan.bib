@Article{Li2010,
  Title                    = {Building the sequence map of the human pan-genome},
  Author                   = {Li, Ruiqiang and Li, Yingrui and Zheng, Hancheng and Luo, Ruibang and Zhu, Hongmei and Li, Qibin and Qian, Wubin and Ren, Yuanyuan and Tian, Geng and Li, Jinxiang and Zhou, Guangyu and Zhu, Xuan and Wu, Honglong and Qin, Junjie and Jin, Xin and Li, Dongfang and Cao, Hongzhi and Hu, Xueda and Blanche, H{\'{e}}l{\`{e}}ne and Cann, Howard and Zhang, Xiuqing and Li, Songgang and Bolund, Lars and Kristiansen, Karsten and Yang, Huanming and Wang, Jun and Wang, Jian},
