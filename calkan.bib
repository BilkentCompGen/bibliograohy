  timestamp   = {2015.09.10},
}

@Article{Weese2012,
  author      = {Weese, David and Holtgrewe, Manuel and Reinert, Knut},
  title       = {{RazerS} 3: faster, fully sensitive read mapping},
  year        = {2012},
  language    = {eng},
  volume      = {28},
