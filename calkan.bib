  timestamp       = {2019.10.15},
}

@Article{Hassoun2017,
  author          = {Hassoun, Ali and Linden, Peter K and Friedman, Bruce},
  title           = {Incidence, prevalence, and management of {MRSA} bacteremia across patient populations-a review of recent developments in {MRSA} management and treatment.},
  journal         = {Critical care (London, England)},
  year            = {2017},
  volume          = {21},
