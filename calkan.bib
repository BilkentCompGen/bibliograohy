  issn-linking    = {1367-4803},
  keywords        = {Algorithms; Base Sequence; Comparative Genomic Hybridization; DNA, chemistry, classification; Genetic Variation; Genome, Human; Genomics, methods; Humans},
  nlm             = {PMC2687962},
