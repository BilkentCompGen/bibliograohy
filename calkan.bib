}

@Article{Weischenfeldt2013,
  Title                    = {Phenotypic impact of genomic structural variation: insights from and for human disease},
  Author                   = {Weischenfeldt, Joachim and Symmons, Orsolya and Spitz, Fran{\c{c}}ois and Korbel, Jan O.},
