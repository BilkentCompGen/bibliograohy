@Article{Evrony2012,
  author          = {Evrony, Gilad D and Cai, Xuyu and Lee, Eunjung and Hills, L Benjamin and Elhosary, Princess C and Lehmann, Hillel S and Parker, J J and Atabay, Kutay D and Gilmore, Edward C and Poduri, Annapurna and Park, Peter J and Walsh, Christopher A},
  title           = {Single-neuron sequencing analysis of {L1} retrotransposition and somatic mutation in the human brain},
