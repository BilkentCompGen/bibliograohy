  keywords        = {DNA, Bacterial, genetics; Escherichia coli, classification, genetics; Evolution, Molecular; Gene Expression Regulation, Bacterial; Genome, Bacterial; Multigene Family; Phylogeny; Plasmids, genetics; Sequence Analysis, DNA},
  month           = dec,
