@Article{Weirather2017,
  author       = {Weirather, Jason L and de Cesare, Mariateresa and Wang, Yunhao and Piazza, Paolo and Sebastiano, Vittorio and Wang, Xiu-Jie and Buck, David and Au, Kin Fai},
