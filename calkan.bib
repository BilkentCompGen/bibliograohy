  Author                   = {Bamshad, Michael J. and Ng, Sarah B. and Bigham, Abigail W. and Tabor, Holly K. and Emond, Mary J. and Nickerson, Deborah A. and Shendure, Jay},
  Journal                  = {Nat Rev Genet},
