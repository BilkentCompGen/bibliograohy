  Institution              = {454 Life Sciences Corp., 20 Commercial Street, Branford, Connecticut 06405, USA.},
  Keywords                 = {Electrophoresis, Capillary; Emulsions; Fiber Optic Technology; Genome, Bacterial; Genomics, economics/instrumentation; Microchemistry, economics/instrumentation; Mycoplasma genitalium, genetics; Polymerase Chain Reaction; Reproducibility of Results; Sensitivity and Specificity; Sequence Analysis, DNA, economics/instrumentation; Time Factors},
