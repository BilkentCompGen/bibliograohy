@Article{Goldstein2013,
  Title                    = {Sequencing studies in human genetics: design and interpretation},
  Author                   = {Goldstein, David B. and Allen, Andrew and Keebler, Jonathan and Margulies, Elliott H. and Petrou, Steven and Petrovski, Slav{\'{e}} and Sunyaev, Shamil},
