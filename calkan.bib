@Article{Antipov2016,
  author      = {Antipov, Dmitry and Korobeynikov, Anton and McLean, Jeffrey S. and Pevzner, Pavel A.},
  title       = {{hybridSPAdes}: an algorithm for hybrid assembly of short and long reads},
  year        = {2016},
  language    = {eng},
