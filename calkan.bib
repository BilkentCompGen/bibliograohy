}

@Article{Li2018,
  author       = {Li, Heng},
  title        = {Minimap2: pairwise alignment for nucleotide sequences.},
  journal      = {Bioinformatics (Oxford, England)},
  year         = {2018},
  volume       = {34},
  issue        = {18},
  month        = sep,
  pages        = {3094--3100},
