@Article{Kucuk2015,
  Title                    = {Activating mutations of {STAT5B} and {STAT3} in lymphomas derived from $\gamma$$\delta$-T or NK cells},
  Author                   = {K{\"{u}}{\c{c}}{\"{u}}k, Can and Jiang, Bei and Hu, Xiaozhou and Zhang, Wenyan and Chan, John K C. and Xiao, Wenming and Lack, Nathan and Alkan, Can and Williams, John C. and Avery, Kendra N. and Kavak, P{\i}nar and Scuto, Anna and Sen, Emel and Gaulard, Philippe and Staudt, Lou and Iqbal, Javeed and Zhang, Weiwei and Cornish, Adam and Gong, Qiang and Yang, Qunpei and Sun, Hong and d'Amore, Francesco and Lepp{\"{a}}, Sirpa and Liu, Weiping and Fu, Kai and {de Leval}, Laurence and McKeithan, Timothy and Chan, Wing C.},
  Journal                  = {Nat Commun},
  Year                     = {2015},
