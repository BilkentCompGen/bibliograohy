@Article{Brandler2016,
  Title                    = {Frequency and Complexity of De Novo Structural Mutation in Autism},
  Author                   = {Brandler, William M. and Antaki, Danny and Gujral, Madhusudan and Noor, Amina and Rosanio, Gabriel and Chapman, Timothy R. and Barrera, Daniel J. and Lin, Guan Ning and Malhotra, Dheeraj and Watts, Amanda C. and Wong, Lawrence C. and Estabillo, Jasper A. and Gadomski, Therese E. and Hong, Oanh and Fajardo, Karin V Fuentes and Bhandari, Abhishek and Owen, Renius and Baughn, Michael and Yuan, Jeffrey and Solomon, Terry and Moyzis, Alexandra G. and Maile, Michelle S. and Sanders, Stephan J. and Reiner, Gail E. and Vaux, Keith K. and Strom, Charles M. and Zhang, Kang and Muotri, Alysson R. and Akshoomoff, Natacha and Leal, Suzanne M. and Pierce, Karen and Courchesne, Eric and Iakoucheva, Lilia M. and Corsello, Christina and Sebat, Jonathan},
  Journal                  = {Am J Hum Genet},
  Year                     = {2016},

  Month                    = {Apr},
  Number                   = {4},
  Pages                    = {667--679},
  Volume                   = {98},

  Abstract                 = {Genetic studies of autism spectrum disorder (ASD) have established that de novo duplications and deletions contribute to risk. However, ascertainment of structural variants (SVs) has been restricted by the coarse resolution of current approaches. By applying a custom pipeline for SV discovery, genotyping, and de novo assembly to genome sequencing of 235 subjects (71 affected individuals, 26 healthy siblings, and their parents), we compiled an atlas of 29,719 SV loci (5,213/genome), comprising 11 different classes. We found a high diversity of de novo mutations, the majority of which were undetectable by previous methods. In addition, we observed complex mutation clusters where combinations of de novo SVs, nucleotide substitutions, and indels occurred as a single event. We estimate a high rate of structural mutation in humans (20\%) and propose that genetic risk for ASD is attributable to an elevated frequency of gene-disrupting de novo SVs, but not an elevated rate of genome rearrangement.},
  Doi                      = {10.1016/j.ajhg.2016.02.018},
  Institution              = {Beyster Center for Genomics of Psychiatric Diseases, University of California, San Diego, La Jolla, CA 92093, USA; Department of Psychiatry, University of California, San Diego, La Jolla, CA 92093, USA; Department of Cellular and Molecular Medicine, University of California, San Diego, La Jolla, CA 92093, USA. Electronic address: jsebat@ucsd.edu.},
