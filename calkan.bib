@Article{Nuttle2013,
  Title                    = {Rapid and accurate large-scale genotyping of duplicated genes and discovery of interlocus gene conversions},
  Author                   = {Nuttle, Xander and Huddleston, John and O'Roak, Brian J. and Antonacci, Francesca and Fichera, Marco and Romano, Corrado and Shendure, Jay and Eichler, Evan E.},
  Journal                  = {Nat Methods},
