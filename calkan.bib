  Institution              = {Department of Biostatistics, Johns Hopkins Bloomberg School of Public Health, Baltimore, MD 21205, USA.},
  Keywords                 = {Models, Statistical; Oligonucleotide Array Sequence Analysis; Polymorphism, Single Nucleotide; Software},
