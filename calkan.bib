@Article{Spies2017,
  author          = {Spies, Noah and Weng, Ziming and Bishara, Alex and McDaniel, Jennifer and Catoe, David and Zook, Justin M and Salit, Marc and West, Robert B and Batzoglou, Serafim and Sidow, Arend},
