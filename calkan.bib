  Institution              = {Program in Medical and Population Genetics, Broad Institute of Harvard and MIT, Cambridge, Massachusetts, USA. depristo@broadinstitute.org},
  Keywords                 = {Data Interpretation, Statistical; Databases, Nucleic Acid; Exons; Genetic Variation; Genetics, Population, methods/statistics /&/ numerical data; Genome, Human; Genotype; Humans; Polymorphism, Single Nucleotide; Sequence Alignment, methods/statistics /&/ numerical data; Sequence Analysis, DNA, methods/statistics /&/ numerical data; Software},
