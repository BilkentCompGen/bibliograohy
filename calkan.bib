@Article{Au2012,
  author          = {Au, Kin Fai and Underwood, Jason G and Lee, Lawrence and Wong, Wing Hung},
  title           = {Improving {PacBio} long read accuracy by short read alignment},
  journal         = {PloS one},
  year            = {2012},
  volume          = {7},
  issue           = {10},
